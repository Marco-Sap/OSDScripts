	</gs:MUILanguagePreferences>
	<!-- input preferences -->
	<gs:InputPreferences>
    <!--en-US-->
    <gs:InputLanguageID Action="add" ID="0413:00020409" Default="true"/> 
	</gs:InputPreferences>
</gs:GlobalizationServices>